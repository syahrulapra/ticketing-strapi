{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "subject": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "open",
        "answered",
        "closed"
      ],
      "default": "open"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high"
      ],
      "default": "low",
      "required": true
    }
  }
}
